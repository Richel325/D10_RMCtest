{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}

<div class="search-api-autocomplete-suggestion">
    <span class="autocomplete-suggestion-label">
        {{ name }}
        <span class="badge text-bg-light bg-opacity-50 me-2 my-1">Video</span>
        <span class="badge text-bg-light bg-opacity-50 me-2 my-1">{{ content.field_group.0 }}</span>
    </span>
</div>
